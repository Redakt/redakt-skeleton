{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Redakt": {
    "LicenseKey": "<your Redakt license key>",
    "DataStore": {
      "AzureCosmosDb": {
        "EndpointUri": "<your cosmosdb endpoint uri>",
        "PrimaryKey": "<your cosmosdb primary key>"
      }
    },
    "FileStore": {
      "AzureStorage": {
        "ConnectionString": "<your azure storage connection string>"
      }
    },
    "ApplicationCache": {
      "InMemory": {
        "SlidingExpirationSeconds": 300,
        "AbsoluteExpirationSeconds": 3600
      }
    }
  }
}