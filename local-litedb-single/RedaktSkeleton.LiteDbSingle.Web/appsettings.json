{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Redakt": {
    "LicenseKey": "<your Redakt license key>",
    "DataStore": {
      "LiteDB": {
        "FilePath": "App_Data\\redakt.db"
      }
    },
    "ApplicationCache": {
      "InMemory": {
        "SlidingExpirationSeconds": 300,
        "AbsoluteExpirationSeconds": 3600
      }
    }
  }
}
