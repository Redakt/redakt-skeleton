{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Redakt": {
    "LicenseKey": "<your Redakt license key>",
    "DataStore": {
      "LiteDB": {
        "FilePath": "App_Data\\redakt.db"
      }
    },
    "ApplicationCache": {
      "InMemory": {
        "SlidingExpirationSeconds": 300,
        "AbsoluteExpirationSeconds": 3600
      }
    },
    "ContentManagement": {
      "Media": {
        "RootUrl": "/media",
        "CacheControlMaxAge": 31536000,
        "ImageCachePath": "App_Data\\Media\\Cache"
      },
      "IgnorePaths": [
        "/media"
      ]
    }
  }
}
